
public class Hopscotch {
	public static void main(String []arg){
		for(int i = 0; i < 1000; i++){
			if(isPrime(i) == true)
				System.out.println(i);
		}
	}
	
	static boolean isPrime(int num){
		if (num % 2 == 0)
			return false;
		
		else
			for(int i = 1; i < num-1; i += 2){
				if(num % i == 0)
					return false;
			}
		
		return true;
	}
}
